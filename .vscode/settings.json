{
  "files.eol": "\n",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true,
  "editor.rulers": [
    50,
    80,
    100
  ],
  "spellright.language": [
    "en"
  ],
  "spellright.documentTypes": [
    "plaintext",
    "markdown"
  ],
  "recommendations": [
    "dbaeumer.vscode-eslint"
  ]
}